    <!-- http://www.w3.org/ns/lemon/vartrans#ConceptualRelation -->

    <owl:Class rdf:about="&vartrans;ConceptualRelation">
        <rdfs:label xml:lang="de">Konzeptbeziehung</rdfs:label>
        <rdfs:label xml:lang="en">conceptual relation</rdfs:label>
        <rdfs:label xml:lang="es">relación entre conceptos</rdfs:label>
        <rdfs:label xml:lang="it">relazione di concetti</rdfs:label>
        <rdfs:label xml:lang="nl">conceptrelatie</rdfs:label>
        <rdfs:label xml:lang="ru">отношение понятий</rdfs:label>
        <rdfs:subClassOf rdf:resource="&vartrans;LexicoSemanticRelation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&vartrans;relates"/>
                <owl:onClass rdf:resource="&ontolex;LexicalConcept"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A relationship between two concepts</rdfs:comment>
        <rdfs:comment xml:lang="ru">Отношение между двумя понятиями</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/lemon/vartrans#LexicalRelation -->

    <owl:Class rdf:about="&vartrans;LexicalRelation">
        <rdfs:label xml:lang="af">leksikale relasie</rdfs:label>
        <rdfs:label xml:lang="de">lexikalische Beziehung</rdfs:label>
        <rdfs:label xml:lang="en">lexical relation</rdfs:label>
        <rdfs:label xml:lang="es">relación léxica</rdfs:label>
        <rdfs:label xml:lang="fr">relation lexicale</rdfs:label>
        <rdfs:label xml:lang="it">relazione lessicale</rdfs:label>
        <rdfs:label xml:lang="nl">lexikaal relatie</rdfs:label>
        <rdfs:label xml:lang="pt">relação semantica</rdfs:label>
        <rdfs:label xml:lang="ro">relație lexicală</rdfs:label>
        <rdfs:label xml:lang="sv">lexikalisk relation</rdfs:label>
        <rdfs:label xml:lang="ru">лексическое отношение</rdfs:label>
        <rdfs:subClassOf rdf:resource="&vartrans;LexicoSemanticRelation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&vartrans;relates"/>
                <owl:onClass rdf:resource="&ontolex;LexicalEntry"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A &apos;lexical relation&apos; is a lexico-semantic relation that represents the relation between two lexical entries the surface forms of which are related grammatically, stylistically or by some operation motivated by linguistic economy.</rdfs:comment>
        <rdfs:comment xml:lang="ru">&apos;Лексическое отношение&apos; - это лексико-семантическое отношение, которое представляет собой отношение между двумя словарными единицами, поверхностные формы которых связаны грамматически, стилистически или неким процессом, обусловленным лингвистической экономией.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/lemon/vartrans"/>
        <vs:term_status>stable</vs:term_status>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/lemon/vartrans#LexicoSemanticRelation -->

    <owl:Class rdf:about="&vartrans;LexicoSemanticRelation">
